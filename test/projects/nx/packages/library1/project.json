{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "name": "library1",
  "sourceRoot": "packages/library1/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/library1",
        "main": "packages/library1/src/index.ts",
        "tsConfig": "packages/library1/tsconfig.lib.json",
        "assets": ["packages/library1/*.md"],
        "transformers": [
          {
            "name": "typescript-transform-paths/plugins/nx",
            "options": {
              "afterDeclarations": true
            }
          }
        ]
      }
    }
  },
  "tags": []
}
